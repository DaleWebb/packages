{
  "name": "@jeromefitz/utils",
  "version": "0.0.0",
  "description": "Utilities used by @JeromeFitz that may be helpful",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "keywords": [
    "utils",
    "utilities"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/utils"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "tsup-node src/*",
    "dev": "tsup-node src/* --watch",
    "---": "",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "----": "",
    "semantic-release": "yarn copy && semantic-release",
    "semantic-release:mono": "yarn copy && semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "4.14.178"
  },
  "type": "module",
  "sideEffects": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js"
}
